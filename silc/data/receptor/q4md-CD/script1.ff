        { "HO"  "H" "sp3" }
        { "H1"  "H" "sp3" }
        { "H2"  "H" "sp3" }
        { "HC"  "H" "sp3" }
        { "HA"  "H" "sp3" }
        { "HW"  "H" "sp3" }
        { "OW"  "O" "sp3" }
        { "OH"  "O" "sp3" }
        { "OS"  "O" "sp3" }
        { "O"   "O" "sp2" }
        { "O2"  "O" "sp2" }
        { "S"   "S" "sp3" }
